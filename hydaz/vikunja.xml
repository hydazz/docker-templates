<?xml version="1.0"?>
<Container version="2">
     <Name>vikunja</Name>
     <Beta>False</Beta>
     <Repository>vcxpz/vikunja</Repository>
     <Registry>https://hub.docker.com/r/vcxpz/vikunja/</Registry>
     <Network>bridge</Network>
     <Privileged>false</Privileged>
     <Project>https://vikunja.io/</Project>
     <Support>https://github.com/hydazz/docker-vikunja/issues/new/</Support>
     <Category>Status:Stable</Category>
     <WebUI>http://[IP]:[PORT:80]</WebUI>
     <TemplateURL>https://raw.githubusercontent.com/hydazz/docker-templates/main/hydaz/vikunja.xml</TemplateURL>
     <Icon>https://raw.githubusercontent.com/hydazz/docker-templates/main/hydaz/png/vikunja.png</Icon>
     <Overview>[p][span style="color: #777;"]The Todo-app to organize your life.[/span][/p]</Overview>
     <Config Name="WebUI Port" Target="80" Default="80" Mode="tcp" Description="Container Port: 80" Type="Port" Display="always" Required="true" Mask="false"/>
     <Config Name="API Port" Target="3456" Default="3456" Mode="tcp" Description="Container Port: 3456" Type="Port" Display="always" Required="true" Mask="false"/>
     <Config Name="Debug" Target="DEBUG" Default="true|false" Mode="" Description="Choose wether to display all app logs in the docker logs" Type="Variable" Display="always" Required="false" Mask="false"/>
     <Config Name="Appdata Path" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false"/>
     <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="UID for permissions. Do not change unless you know what you're doing." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
     <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="GID for permissions. Do not change unless you know what you're doing." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
</Container>
