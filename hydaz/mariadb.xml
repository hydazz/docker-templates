<?xml version="1.0"?>
<Container version="2">
     <Name>mariadb</Name>
     <Beta>False</Beta>
     <Repository>vcxpz/mariadb</Repository>
     <Registry>https://hub.docker.com/r/vcxpz/mariadb/</Registry>
     <Network>bridge</Network>
     <Privileged>false</Privileged>
     <Project>https://mariadb.org/</Project>
     <Support>https://github.com/hydazz/docker-mariadb/issues/new/</Support>
     <Category>Network:Management Network:Other Status:Stable</Category>
     <WebUI/>
     <TemplateURL>https://raw.githubusercontent.com/hydazz/docker-templates/main/hydaz/mariadb.xml</TemplateURL>
     <Icon>https://raw.githubusercontent.com/hydazz/docker-templates/main/hydaz/png/mariadb.png</Icon>
     <Overview>[p][span style="color: #777;"]MariaDB was designed as a drop-in replacement of MySQL with more features, new storage engines, fewer bugs, and better performance.[/span][/p]</Overview>
     <Config Name="MariaDB Port" Target="3306" Default="3306" Mode="tcp" Description="Container Port: 3306" Type="Port" Display="always" Required="true" Mask="false"/>
     <Config Name="MySQL Root Password" Target="MYSQL_ROOT_PASSWORD" Default="" Mode="" Description="You can clear this variable after initial startup" Type="Variable" Display="always" Required="false" Mask="true"/>
     <Config Name="Appdata Path" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false"/>
     <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="UID for permissions. Do not change unless you know what you're doing." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
     <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="GID for permissions. Do not change unless you know what you're doing." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
</Container>
