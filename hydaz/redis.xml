<?xml version="1.0"?>
<Container version="2">
     <Name>redis</Name>
     <Beta>False</Beta>
     <Repository>vcxpz/redis</Repository>
     <Registry>https://hub.docker.com/r/vcxpz/redis/</Registry>
     <Network>bridge</Network>
     <Privileged>false</Privileged>
     <Project>https://redis.io/</Project>
     <Support>https://github.com/hydazz/docker-redis/issues/new/</Support>
     <Category>Network:Management Status:Stable</Category>
     <WebUI/>
     <TemplateURL>https://raw.githubusercontent.com/hydazz/docker-templates/main/hydaz/redis.xml</TemplateURL>
     <Icon>https://raw.githubusercontent.com/hydazz/docker-templates/main/hydaz/png/redis.png</Icon>
     <Overview>[p][span style="color: #777;"]Redis is an in-memory data structure store, used as a distributed, in-memory key-value database, cache and message broker, with optional durability.[/span][/p]</Overview>
     <Config Name="Redis Port" Target="6379" Default="6379" Mode="tcp" Description="Container Port: 6379" Type="Port" Display="always-hide" Required="true" Mask="false"/>
     <Config Name="Appdata Path" Target="/config" Default="" Mode="rw" Description="Container Path: /config" Type="Path" Display="advanced-hide" Required="true" Mask="false"/>
     <Config Name="Debug" Target="DEBUG" Default="true|false" Mode="" Description="Choose wether to display all app logs in the docker logs" Type="Variable" Display="always" Required="false" Mask="false"/>
     <Config Name="PUID" Target="PUID" Default="99" Mode="" Description="UID for permissions. Do not change unless you know what you're doing." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
     <Config Name="PGID" Target="PGID" Default="100" Mode="" Description="GID for permissions. Do not change unless you know what you're doing." Type="Variable" Display="advanced-hide" Required="false" Mask="false"/>
</Container>
